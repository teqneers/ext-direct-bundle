{
    "name": "teqneers/ext-direct-bundle",
    "type": "library",
    "description": "A Symfony bundle to integrate Sencha Ext JS Ext.direct into a Symfony application",
    "homepage": "https://github.com/teqneers/ext-direct-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Gehrig",
            "email": "gehrig@teqneers.de",
            "homepage": "https://www.teqneers.de/"
        },
        {
            "name": "Erik Theoboldt",
            "email": "theoboldt@teqneers.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "TQ\\Bundle\\ExtDirectBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TQ\\Bundle\\ExtDirectBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2.0",
        "jms/serializer-bundle": ">=3.2.0",
        "symfony/config": "~3.0 || ~4.0 || ~5.0",
        "symfony/dependency-injection": "~3.0 || ~4.0 || ~5.0",
        "symfony/http-kernel": "~3.0 || ~4.0 || ~5.0",
        "symfony/routing": "~3.0 || ~4.0 || ~5.0",
        "symfony/yaml": "~3.0 || ~4.0 || ~5.0",
        "teqneers/ext-direct": "3.1.x-dev",
        "twig/twig": "~1.24 || ~2.0 || ~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/framework-bundle": "~3.0 || ~4.0 || ~5.0",
        "symfony/phpunit-bridge": "~5.0",
        "symfony/security-bundle": "~3.0 || ~4.0 || ~5.0",
        "symfony/templating": "~3.0 || ~4.0 || ~5.0"
    }
}
